<!--====== Category Part Ends ======-->
<!--====== Product Part Start ======-->

<section class="product-area pt-100 pb-100">
    <div class="container-fluid custom-container">
        <div class="row">
            <div class="col-lg-12">
                <div class="product-menu pb-30">
                    <ul class="nav justify-content-center" id="myTab" role="tablist">
                        @for (int i = 0; i < Model.Categories.Take(4).ToList().Count; i++)
                        {
                            <li class="nav-item" role="presentation">
                                <a class="@(i==0?"active":"")" id="new-tab" data-toggle="tab" href="#new" role="tab" aria-controls="new" aria-selected="@(i==0?true:false)">@Model.Categories[i].CategoryName</a>
                            </li>
                        }

                    </ul>
                </div> <!-- product menu -->
            </div>
        </div> <!-- row -->
        <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade show active" id="new" role="tabpanel" aria-labelledby="new-tab">
                <div class="product-items">
                    <div class="row product-active">
                        @for (int i = 0; i < Model.Products.Count - 1; i += 2)
                        {
                            <div class="col-lg-2">
                                <div class="single-product mt-30">
                                    <div class="product-image">
                                        <a href="single-product.html">
                                            <img style="width:350px;overflow:hidden;" class="first-image" src="~/assets/images/@Model.Products[i+1].ProductImages.FirstOrDefault(pi=>pi.IsMain).ImageUrl" alt="product">
                                            <img style="width:350px;overflow:hidden;" class="second-image" src="~/assets/images/@Model.Products[i+1].ProductImages.FirstOrDefault(pi=>pi.IsHover).ImageUrl" alt="product">
                                        </a>
                                        <ul class="product-meta">
                                            <li><a href="#"><i class="icon ion-bag"></i></a></li>
                                            <li><a href="#"><i class="icon ion-android-favorite-outline"></i></a></li>
                                            <li><a href="#"><i class="icon ion-android-options"></i></a></li>
                                            <li><a data-toggle="modal" data-target="#productQuickModal" href="#"><i class="icon ion-android-open"></i></a></li>
                                        </ul>
                                        @if (Model.Products[i].SalePercentage > 0)
                                            {
                                                 <span class="discount-product">-@Model.Products[i+1].SalePercentage %</span>
                                            }
                                    </div>
                                    <div class="product-content">
                                        <ul class="product-rating">
                                            @for (int j = 0; j < 5; j++)
                                            {
                                                if (j < Model.Products[i + 1].StarsCount)
                                                {
                                                    <li><i style="color:orange;" class="fas fa-star"></i></li>
                                                }
                                                else
                                                {
                                                    <li><i class="fas fa-star"></i></li>
                                                }

                                            }
                                        </ul>
                                        <h4 class="product-title"><a href="single-product.html">@Model.Products[i+1].Name</a></h4>
                                        <div class="product-price">
                                            <span class="price-sale">€@(Model.Products[i+1].Price- Model.Products[i+1].Price* Model.Products[i+1].SalePercentage/100)</span>
                                        </div>
                                    </div>
                                </div> <!-- single product -->
                                <div class="single-product mt-30">
                                    <div class="product-image">
                                        <a href="single-product.html">
                                            <img style="width:350px;overflow:hidden;" class="first-image" src="~/assets/images/@Model.Products[i].ProductImages.FirstOrDefault(pi=>pi.IsMain).ImageUrl" alt="product">
                                            <img style="width:350px;overflow:hidden;" class="second-image" src="~/assets/images/@Model.Products[i].ProductImages.FirstOrDefault(pi=>pi.IsHover).ImageUrl" alt="product">
                                        </a>
                                        <ul class="product-meta">
                                            <li><a href="#"><i class="icon ion-bag"></i></a></li>
                                            <li><a href="#"><i class="icon ion-android-favorite-outline"></i></a></li>
                                            <li><a href="#"><i class="icon ion-android-options"></i></a></li>
                                            <li><a data-toggle="modal" data-target="#productQuickModal" href="#"><i class="icon ion-android-open"></i></a></li>
                                        </ul>
                                        @if (Model.Products[i].SalePercentage > 0)
                                        {
                                            <span class="discount-product">-@Model.Products[i].SalePercentage %</span>
                                        }
                                    </div>
                                    <div class="product-content">
                                        <ul class="product-rating">
                                            @for (int j = 0; j < 5; j++)
                                            {
                                                if (j < Model.Products[i].StarsCount)
                                                {
                                                    <li><i style="color:orange;" class="fas fa-star"></i></li>
                                                }
                                                else
                                                {
                                                    <li><i class="fas fa-star"></i></li>
                                                }

                                            }
                                        </ul>
                                        <h4 class="product-title"><a href="single-product.html">@Model.Products[i].Name</a></h4>
                                        <div class="product-price">
                                            <span class="price-sale">€@(Model.Products[i].Price- Model.Products[i].Price* Model.Products[i].SalePercentage/100)</span>
                                        </div>
                                    </div>
                                </div> <!-- single product -->
                            </div>
                        }
                    </div> <!-- row -->
                </div> <!-- product items -->
            </div>
        </div> <!-- tab content -->
    </div> <!-- container -->
</section>

<!--====== Product Part Ends ======-->
